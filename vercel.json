{
  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "astropowermap.com" }],
      "destination": "https://www.astropowermap.com/:path*",
      "permanent": true
    }
  ],
  "crons": [
    {
      "path": "/api/cron/classify-chats",
      "schedule": "0 6 * * *"
    }
  ]
}
